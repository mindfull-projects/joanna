{
    "name": "joanna-website",
    "version": "0.1.0",
    "description": "Joanna personal website",
    "private": true,
    "scripts": {
        "start": "npx sst dev",
        "dev": "next dev --turbopack",
        "build": "next build",
        "lint": "next lint",
        "deploy": "AWS_PROFILE=mindfull npx sst deploy --stage production",
        "destroy": "AWS_PROFILE=mindfull npx sst remove --stage production",
        "aws:login": "aws sso login --profile mindfull",
        "test": "echo \"No test specified\"",
        "prepare": "husky"
    },
    "dependencies": {
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "next": "15.2.4"
    },
    "devDependencies": {
        "typescript": "^5",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "@tailwindcss/postcss": "^4",
        "tailwindcss": "^4",
        "eslint": "^9",
        "eslint-config-next": "15.2.4",
        "@eslint/eslintrc": "^3",
        "husky": "^9.1.7",
        "lint-staged": "^15.3.0",
        "prettier": "^3.4.2",
        "prettier-plugin-tailwindcss": "^0.6.9"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": "eslint --fix",
        "*.{js,jsx,ts,tsx,md}": "prettier --write"
    }
}
