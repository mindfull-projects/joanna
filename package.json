{
  "name": "joanna-website",
  "version": "0.1.0",
  "description": "Joanna personal website",
  "private": true,
  "scripts": {
    "start": "sst dev",
    "dev": "next dev --turbopack",
    "build": "next build",
    "lint": "next lint",
